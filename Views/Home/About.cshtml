@{
    ViewBag.Title = Resource.L_Menu_AboutUs;
}


<section class="section-40 section-md-60 section-lg-90 section-xl-120 bg-gray-dark page-title-wrap overlay-5 custom-bg-image" style="background-image: url(@Url.Content("~/Template/images/bg-image-1.jpg"));">
    <div class="container">
        <div class="page-title text-center">
            <h2>@Resource.L_Menu_AboutUs</h2>
        </div>
    </div>
</section>

<section class="section-66 section-md-90 section-xl-bottom-120 bg-whisperapprox novi-background bg-cover">
    <div class="container">
        <h3>@Resource.AboutSMG_Title</h3>
        <div class="row row-40 justify-content-xl-between align-items-center">
            <div class="col-md-6 col-xl-5 text-secondary">
                <div class="inset-md-right-15 inset-xl-right-0">
                    <p>
                        @Resource.AboutSMG_Desc_1
                    </p>
                    <p>
                        @Resource.AboutSMG_Desc_2
                    </p>
                    <p>
                        @Resource.AboutSMG_Desc_3
                    </p>
                </div>
            </div>
            <div class="col-md-6   align-items-end context-dark overlay-9" style="background-image: url(@Url.Content("~/Template/images/bg-image-6.jpg"))">
                <div class="owl-item active" style="margin: 60px 20px 60px 20px;">
                    <div class="inset-xl-left-60 inset-xl-right-40">
                        <blockquote class="quote-bordered">
                            <div class="quote-body">
                                <div class="quote-open fa fa-quote-left novi-icon"></div>
                                <div class="quote-body-inner">
                                    <h6>@Resource.CEOMessage_Title</h6>
                                    <p class="text-uppercase">
                                        <q>@Resource.CEOMessage_Desc_1 <b>@Resource.CEOMEssage_Desc_2</b></q>
                                    </p>
                                </div>
                            </div>
                            <div class="quote-footer">
                                <div class="unit flex-row unit-spacing-sm align-items-center">
                                    <div class="unit-left">
                                        <img class="img-circle" style="width:100px" src="@Url.Content("~/Template/images/AmrWagih_Face.jpeg")" width="68" height="68" alt="">
                                    </div>
                                    <div class="unit-body">
                                        <cite class="text-white">@Resource.MrAmrWagih</cite>
                                        <p class="text-primary">@Resource.CEO</p>
                                    </div>
                                </div>
                            </div>
                        </blockquote>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


